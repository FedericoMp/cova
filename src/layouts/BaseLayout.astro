---
interface Props {
	title: string;
	headerBlackBg?: boolean;
	headerFixedTop?: boolean;
}
const { title, headerBlackBg, headerFixedTop } = Astro.props;
import Layout from "./Layout.astro";
---

<Layout title={title} headerBlackBg={headerBlackBg} headerFixedTop={headerFixedTop}>
    <slot />
</Layout>
